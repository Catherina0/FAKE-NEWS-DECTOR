def validate_score(score: Any, source: str = "æœªçŸ¥") -> float:
    """éªŒè¯å¹¶è½¬æ¢è¯„åˆ†"""
    try:
        score_float = float(score)
        if not 0 <= score_float <= 1:
            logger.warning(f"è¯„åˆ†è¶…å‡ºèŒƒå›´[0-1]: {score_float} (æ¥æº: {source})")
            return max(0.0, min(1.0, score_float))
        return score_float
    except (TypeError, ValueError) as e:
        logger.error(f"è¯„åˆ†è½¬æ¢å¤±è´¥: {score} (æ¥æº: {source}) - {str(e)}")
        raise ValueError(f"æ— æ•ˆçš„è¯„åˆ†å€¼: {score}")

def validate_data(data: Dict[str, Any], required_fields: list, context: str = "") -> bool:
    """éªŒè¯æ•°æ®å®Œæ•´æ€§"""
    missing_fields = [field for field in required_fields if field not in data]
    if missing_fields:
        logger.warning(f"{context} - ç¼ºå¤±å­—æ®µ: {', '.join(missing_fields)}")
        return False
    return True

def calculate_weighted_score(main_scores: dict, cross_validation_data: dict = None) -> Tuple[float, dict]:
    """
    è®¡ç®—ä¸»è¦å„é¡¹è¯„åˆ†çš„åŠ æƒæ€»åˆ†å’Œå®é™…ä½¿ç”¨çš„æƒé‡
    
    å‚æ•°:
        main_scores (dict): å„é¡¹è¯„åˆ†æ•°æ®
        cross_validation_data (dict, optional): äº¤å‰éªŒè¯æ•°æ®
    
    è¿”å›:
        Tuple[float, dict]: åŠ æƒæ€»åˆ†å’Œä½¿ç”¨çš„æƒé‡
    """
    try:
        # åˆå§‹åŒ–å˜é‡
        weighted_sum = 0.0
        total_weight = 0.0
        used_weights = {}
        
        # é»˜è®¤æƒé‡é…ç½®
        weights = {
            "aiå†…å®¹æ£€æµ‹": 0.2,
            "è¯­è¨€ä¸­ç«‹æ€§": 0.2,
            "æ¥æºå¼•ç”¨è´¨é‡": 0.25,
            "deepseekç»¼åˆåˆ†æ": 0.15,
            "äº¤å‰éªŒè¯": 0.2
        }
        
        # å¤„ç†AIå†…å®¹æ£€æµ‹
        if main_scores and isinstance(main_scores, dict) and "AIå†…å®¹æ£€æµ‹" in main_scores:
            try:
                ai_score = validate_score(main_scores["AIå†…å®¹æ£€æµ‹"], "AIå†…å®¹æ£€æµ‹")
                weighted_sum += ai_score * weights["aiå†…å®¹æ£€æµ‹"]
                total_weight += weights["aiå†…å®¹æ£€æµ‹"]
                used_weights["aiå†…å®¹æ£€æµ‹"] = weights["aiå†…å®¹æ£€æµ‹"]
                logger.debug(f"AIå†…å®¹æ£€æµ‹è¯„åˆ†: {ai_score:.2f}, æƒé‡: {weights['aiå†…å®¹æ£€æµ‹']}")
            except ValueError:
                logger.warning("AIå†…å®¹æ£€æµ‹è¯„åˆ†æ— æ•ˆï¼Œä¸å‚ä¸æ€»åˆ†è®¡ç®—")
        
        # å¤„ç†è¯­è¨€ä¸­ç«‹æ€§
        if main_scores and isinstance(main_scores, dict) and "è¯­è¨€ä¸­ç«‹æ€§" in main_scores:
            try:
                neutrality_score = validate_score(main_scores["è¯­è¨€ä¸­ç«‹æ€§"], "è¯­è¨€ä¸­ç«‹æ€§")
                weighted_sum += neutrality_score * weights["è¯­è¨€ä¸­ç«‹æ€§"]
                total_weight += weights["è¯­è¨€ä¸­ç«‹æ€§"]
                used_weights["è¯­è¨€ä¸­ç«‹æ€§"] = weights["è¯­è¨€ä¸­ç«‹æ€§"]
                logger.debug(f"è¯­è¨€ä¸­ç«‹æ€§è¯„åˆ†: {neutrality_score:.2f}, æƒé‡: {weights['è¯­è¨€ä¸­ç«‹æ€§']}")
            except ValueError:
                logger.warning("è¯­è¨€ä¸­ç«‹æ€§è¯„åˆ†æ— æ•ˆï¼Œä¸å‚ä¸æ€»åˆ†è®¡ç®—")
        
        # å¤„ç†æ¥æºå¼•ç”¨è´¨é‡
        if main_scores and isinstance(main_scores, dict) and "æ¥æºå¼•ç”¨è´¨é‡" in main_scores:
            try:
                source_score = validate_score(main_scores["æ¥æºå¼•ç”¨è´¨é‡"], "æ¥æºå¼•ç”¨è´¨é‡")
                weighted_sum += source_score * weights["æ¥æºå¼•ç”¨è´¨é‡"]
                total_weight += weights["æ¥æºå¼•ç”¨è´¨é‡"]
                used_weights["æ¥æºå¼•ç”¨è´¨é‡"] = weights["æ¥æºå¼•ç”¨è´¨é‡"]
                logger.debug(f"æ¥æºå¼•ç”¨è´¨é‡è¯„åˆ†: {source_score:.2f}, æƒé‡: {weights['æ¥æºå¼•ç”¨è´¨é‡']}")
            except ValueError:
                logger.warning("æ¥æºå¼•ç”¨è´¨é‡è¯„åˆ†æ— æ•ˆï¼Œä¸å‚ä¸æ€»åˆ†è®¡ç®—")
        
        # å¤„ç†DeepSeekç»¼åˆåˆ†æ
        if main_scores and isinstance(main_scores, dict) and "DeepSeekç»¼åˆåˆ†æ" in main_scores:
            try:
                deepseek_score = validate_score(main_scores["DeepSeekç»¼åˆåˆ†æ"], "DeepSeekç»¼åˆåˆ†æ")
                weighted_sum += deepseek_score * weights["deepseekç»¼åˆåˆ†æ"]
                total_weight += weights["deepseekç»¼åˆåˆ†æ"]
                used_weights["deepseekç»¼åˆåˆ†æ"] = weights["deepseekç»¼åˆåˆ†æ"]
                logger.debug(f"DeepSeekç»¼åˆåˆ†æè¯„åˆ†: {deepseek_score:.2f}, æƒé‡: {weights['deepseekç»¼åˆåˆ†æ']}")
            except ValueError:
                logger.warning("DeepSeekç»¼åˆåˆ†æè¯„åˆ†æ— æ•ˆï¼Œä¸å‚ä¸æ€»åˆ†è®¡ç®—")
        
        # å¤„ç†äº¤å‰éªŒè¯
        if cross_validation_data and isinstance(cross_validation_data, dict):
            # å°è¯•ä»ä¸åŒçš„å¯èƒ½å­—æ®µä¸­æå–äº¤å‰éªŒè¯è¯„åˆ†
            cv_score = None
            for key in ["score", "æ€»ä½“å¯ä¿¡åº¦", "overall_score"]:
                if key in cross_validation_data:
                    try:
                        cv_score = validate_score(cross_validation_data[key], f"äº¤å‰éªŒè¯.{key}")
                        logger.info(f"ä»å­—æ®µ {key} ä¸­æå–äº¤å‰éªŒè¯è¯„åˆ†: {cv_score}")
                        break
                    except ValueError:
                        logger.warning(f"äº¤å‰éªŒè¯è¯„åˆ†å­—æ®µ {key} æ— æ•ˆ: {cross_validation_data[key]}")
            
            if cv_score is not None:
                # æ ¹æ®éªŒè¯ç‚¹æƒ…å†µè°ƒæ•´è¯„åˆ†
                if "éªŒè¯ç‚¹" in cross_validation_data and isinstance(cross_validation_data["éªŒè¯ç‚¹"], list):
                    verification_points = cross_validation_data["éªŒè¯ç‚¹"]
                    # ç»Ÿè®¡éªŒè¯ç‚¹ç»“æœ
                    high_score_points = sum(1 for p in verification_points if isinstance(p, dict) and p.get("éªŒè¯è¯„åˆ†", 0) >= 0.7)
                    low_score_points = sum(1 for p in verification_points if isinstance(p, dict) and p.get("éªŒè¯è¯„åˆ†", 0) < 0.4)
                    
                    # æ ¹æ®éªŒè¯ç‚¹è°ƒæ•´è¯„åˆ†
                    if len(verification_points) > 0:
                        success_rate = high_score_points / len(verification_points)
                        fail_rate = low_score_points / len(verification_points)
                        
                        # è°ƒæ•´è¯„åˆ†
                        if success_rate > 0.7:
                            cv_score = min(1.0, cv_score * 1.1)  # æé«˜10%
                            logger.debug(f"å¤§éƒ¨åˆ†éªŒè¯ç‚¹è¯„åˆ†é«˜ï¼Œæå‡äº¤å‰éªŒè¯è¯„åˆ†è‡³: {cv_score}")
                        elif fail_rate > 0.5:
                            cv_score = cv_score * 0.9  # é™ä½10%
                            logger.debug(f"å¤§éƒ¨åˆ†éªŒè¯ç‚¹è¯„åˆ†ä½ï¼Œé™ä½äº¤å‰éªŒè¯è¯„åˆ†è‡³: {cv_score}")
        
        # å¦‚æœæ²¡æœ‰æœ‰æ•ˆçš„è¯„åˆ†é¡¹ï¼Œä½†æœ‰äº¤å‰éªŒè¯æ•°æ®
        if total_weight == 0 and cross_validation_data:
            logger.warning("ä¸»è¦è¯„åˆ†é¡¹ä¸ºç©ºï¼Œå°è¯•å•ç‹¬ä½¿ç”¨äº¤å‰éªŒè¯æ•°æ®")
            try:
                # å°è¯•ä»äº¤å‰éªŒè¯ä¸­æå–è¯„åˆ†
                cv_score = None
                if hasattr(cross_validation_data, 'get'):
                    for key in ["score", "overall_score", "æ€»ä½“å¯ä¿¡åº¦"]:
                        if key in cross_validation_data:
                            try:
                                cv_score = float(cross_validation_data.get(key))
                                logger.info(f"ä»äº¤å‰éªŒè¯ä¸­æå–è¯„åˆ†: {cv_score} (key={key})")
                                break
                            except (ValueError, TypeError):
                                pass
                
                    if cv_score is None:
                        cv_score = 0.7  # é»˜è®¤ä½¿ç”¨0.7
                        logger.info(f"æœªæ‰¾åˆ°æœ‰æ•ˆçš„äº¤å‰éªŒè¯è¯„åˆ†ï¼Œä½¿ç”¨é»˜è®¤å€¼: {cv_score}")
                
                    # å¦‚æœcv_scoreå¤ªå°ï¼Œå¯èƒ½æ˜¯é”™è¯¯å€¼ï¼Œä½¿ç”¨æ›´åˆç†çš„é»˜è®¤å€¼
                    if cv_score < 0.4:
                        cv_score = 0.7
                        logger.warning(f"äº¤å‰éªŒè¯è¯„åˆ†è¿‡ä½ï¼Œä½¿ç”¨é»˜è®¤å€¼: {cv_score}")
                
                    return float(cv_score), {"äº¤å‰éªŒè¯": 1.0}
            except Exception as e:
                logger.error(f"å°è¯•ä½¿ç”¨äº¤å‰éªŒè¯æ•°æ®å¤±è´¥: {str(e)}")
                # å‡ºé”™æ—¶ä½¿ç”¨é»˜è®¤å€¼
                return 0.7, {"äº¤å‰éªŒè¯": 1.0}
        
        # å¦‚æœæ²¡æœ‰æœ‰æ•ˆçš„è¯„åˆ†é¡¹ï¼Œä½¿ç”¨é»˜è®¤åˆ†æ•°
        if total_weight == 0:
            logger.warning("æ²¡æœ‰æœ‰æ•ˆè¯„åˆ†é¡¹ï¼Œä½¿ç”¨é»˜è®¤åˆ†æ•°0.5")
            return 0.5, {"é»˜è®¤åˆ†æ•°": 1.0}
        
        # é‡æ–°å½’ä¸€åŒ–æƒé‡
        if total_weight < 1.0:
            normalization_factor = 1.0 / total_weight
            weighted_sum *= normalization_factor
            used_weights = {k: v * normalization_factor for k, v in used_weights.items()}
        
        logger.info(f"è®¡ç®—å¾—åˆ°åŠ æƒæ€»åˆ†: {weighted_sum:.2f}")
        return weighted_sum, used_weights
    except Exception as e:
        logger.error(f"æ ¼å¼åŒ–ç»“æœæ—¶å‘ç”Ÿé”™è¯¯: {str(e)}")
        import traceback
        logger.error(f"é”™è¯¯è¯¦æƒ…:\n{traceback.format_exc()}")
        print(f"{ERROR_COLOR}æ ¼å¼åŒ–ç»“æœæ—¶å‘ç”Ÿé”™è¯¯: {str(e)}{RESET_COLOR}")
        print(f"{ERROR_COLOR}é”™è¯¯è¯¦æƒ…:\n{traceback.format_exc()}{RESET_COLOR}")
        # å°è¯•æ‰“å°åŸå§‹æ•°æ®ä»¥ä¾¿è°ƒè¯•
        try:
            print(f"{ERROR_COLOR}åŸå§‹æ•°æ®æ‘˜è¦:\næ•°æ®1:{str(main_scores)[:250]}\næ•°æ®2:{str(cross_validation_data)[:250]}{RESET_COLOR}")
        except:
            print(f"{ERROR_COLOR}æ— æ³•æ‰“å°åŸå§‹æ•°æ®{RESET_COLOR}")

def analyze_problems(result: dict, total_score: float, main_scores: dict, cross_validation_data: dict) -> list:
    """
    AIåˆ†æå­˜åœ¨çš„é—®é¢˜
    
    å‚æ•°:
        result: å®Œæ•´çš„åˆ†æç»“æœ
        total_score: æ€»åˆ†
        main_scores: ä¸»è¦ç»´åº¦è¯„åˆ†
        cross_validation_data: äº¤å‰éªŒè¯æ•°æ®
    
    è¿”å›:
        list: é—®é¢˜åˆ—è¡¨ï¼Œæ¯ä¸ªé—®é¢˜æ˜¯ä¸€ä¸ªå­—å…¸ï¼ŒåŒ…å«ä¸¥é‡ç¨‹åº¦ã€æè¿°å’Œå»ºè®®
    """
    problems = []
    
    # 1. åˆ†ææ€»ä½“å¯ä¿¡åº¦
    if total_score < 0.4:
        problems.append({
            "severity": "ä¸¥é‡",
            "type": "æ€»ä½“å¯ä¿¡åº¦",
            "description": f"æ–°é—»æ•´ä½“å¯ä¿¡åº¦æä½ ({total_score:.1%})",
            "suggestion": "å»ºè®®è°¨æ…å¯¹å¾…è¯¥æ–°é—»å†…å®¹ï¼Œéœ€è¦å¤§é‡é¢å¤–éªŒè¯",
            "color": ERROR_COLOR
        })
    elif total_score < 0.6:
        problems.append({
            "severity": "ä¸­ç­‰",
            "type": "æ€»ä½“å¯ä¿¡åº¦",
            "description": f"æ–°é—»å¯ä¿¡åº¦è¾ƒä½ ({total_score:.1%})",
            "suggestion": "å»ºè®®è¿›ä¸€æ­¥æ ¸å®å…³é”®ä¿¡æ¯",
            "color": WARNING_COLOR
        })
    
    # 2. åˆ†æå„ç»´åº¦è¯„åˆ†
    dimension_thresholds = {
        "å†…å®¹çœŸå®æ€§": (0.6, "æ ¸å®æ–°é—»ä¸­çš„å…³é”®äº‹å®å’Œæ•°æ®"),
        "ä¿¡æ¯å‡†ç¡®æ€§": (0.6, "æ£€æŸ¥ä¿¡æ¯çš„å‡†ç¡®æ€§å’Œå®Œæ•´æ€§"),
        "æ¥æºå¯é æ€§": (0.6, "éªŒè¯ä¿¡æ¯æ¥æºçš„æƒå¨æ€§"),
        "å¼•ç”¨è´¨é‡": (0.6, "æ£€æŸ¥å¼•ç”¨çš„å‡†ç¡®æ€§å’Œå¯é æ€§"),
        "è¯­è¨€å®¢è§‚æ€§": (0.5, "æ³¨æ„å¯èƒ½å­˜åœ¨çš„ä¸»è§‚åè§"),
        "é€»è¾‘è¿è´¯æ€§": (0.5, "æ£€æŸ¥å†…å®¹çš„é€»è¾‘æ€§å’Œè¿è´¯æ€§")
    }
    
    for dim, (threshold, suggestion) in dimension_thresholds.items():
        if dim in main_scores:
            score = float(main_scores[dim])
            if score < threshold:
                severity = "ä¸¥é‡" if score < 0.4 else "ä¸­ç­‰"
                problems.append({
                    "severity": severity,
                    "type": dim,
                    "description": f"{dim}è¯„åˆ†è¿‡ä½ ({score:.1%})",
                    "suggestion": suggestion,
                    "color": ERROR_COLOR if score < 0.4 else WARNING_COLOR
                })
    
