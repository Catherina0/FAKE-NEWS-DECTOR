def get_progress_bar(score, width=10):
    """ç”Ÿæˆè¿›åº¦æ¡"""
    if score is None:
        # å¯¹äºNoneå€¼ï¼Œè¿”å›ç©ºè¿›åº¦æ¡
        return f"{'â–‘' * width}"
    
    # ç¡®ä¿scoreæ˜¯æµ®ç‚¹æ•°
    try:
        score_float = float(score)
    except (TypeError, ValueError):
        # å¦‚æœæ— æ³•è½¬æ¢ä¸ºæµ®ç‚¹æ•°ï¼Œè¿”å›ç©ºè¿›åº¦æ¡
        return f"{'â–‘' * width}"
    
